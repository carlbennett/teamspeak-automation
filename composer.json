{
    "name": "carlbennett/ts3bot",
    "description": "Carl's TeamSpeak 3 Bot",
    "type": "project",
    "keywords": [
        "teamspeak",
        "ts3",
        "bot",
        "automation"
    ],
    "homepage": "https://github.com/carlbennett/ts3bot",
    "license": "MIT",
    "authors": [
        {
            "name": "Carl Bennett",
            "email": "carl@carlbennett.me"
        }
    ],
    "config": {
      "vendor-dir": "lib"
    },
    "autoload": {
        "psr-4": {
            "CarlBennett\\TS3Bot\\Commands\\": "src/commands",
            "CarlBennett\\TS3Bot\\Events\\": "src/events",
            "CarlBennett\\TS3Bot\\Libraries\\": "src/libraries"
        }
    },
    "require": {
        "carlbennett/php-mvc": "~2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "php-64bit": ">=5.4.0",
        "planetteamspeak/ts3-php-framework": ">=1.1"
    }
}
